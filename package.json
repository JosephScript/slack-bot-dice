  },
  "main": "lib/dicebot.js",
  "bin": {
    "dicebot": "bin/bot.js"
  },
  "private": true,
  "scripts": {
    "start": "node bin/bot.js"
  },
  "author": {
    "name": "Joseph Szczesniak",
    "email": "nukapunk@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.1"
  },
  "dependencies": {
    "chai": "^3.3.0",
    "debug": "~2.2.0",
    "mocha": "^2.3.3",
    "slackbots": "^0.4.0"
  }
}
